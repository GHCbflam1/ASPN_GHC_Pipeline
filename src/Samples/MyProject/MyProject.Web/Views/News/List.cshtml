@model IEnumerable<MyProject.News.News>
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <div>
        <table>
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Title</td>
                    <td>Intro</td>
                </tr>
            </thead>
            <tbody>
                @if (Model != null && Model.Count() > 0)
                {
                    foreach (var item in Model)
                    { <tr>
                        <td>@item.Id</td>
                        <td>@item.Title</td>
                        <td>@item.Intro</td>
                    </tr>
                    }
                }
                else
                {
                    <tr>
                        <td colspan="3">暂无数据</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
